{
    "name": "arbiter/arbiter",
    "type": "library",
    "description": "A PSR-7 Action system for Action-Domain-Responder.",
    "keywords": [
        "middleware",
        "psr-7",
        "adr",
        "action domain responder"
    ],
    "homepage": "https://github.com/arbiterphp/Arbiter.Arbiter",
    "license": "MIT",
    "authors": [
        {
            "name": "Arbiter.Arbiter Contributors",
            "homepage": "https://github.com/arbiterphp/Arbiter.Arbiter/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "psr/http-message": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Arbiter\\": "src/"
        }
    },
    "require-dev": {
        "zendframework/zend-diactoros": "~1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Arbiter\\": "tests/"
        }
    }
}
